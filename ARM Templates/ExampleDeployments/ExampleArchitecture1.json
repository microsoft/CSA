          },
          "vnet_id": {
            "value": "[reference('deployVNET').outputs.vnetID.value]"
          },
          "recordname": {
            "value": "[parameters('apimname')]"
          },
          "ipaddress": {
            "value": "[reference('deployAPIM').outputs.APIMIP.value[0]]"
